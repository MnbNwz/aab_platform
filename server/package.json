{
  "name": "server",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "dev": "npm cache clean --f && reset && nodemon -r tsconfig-paths/register app.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "seed:admin": "npx ts-node -r tsconfig-paths/register scripts/seedAdmin.ts",
    "seed:memberships": "npx ts-node -r tsconfig-paths/register scripts/seedMembershipPlans.ts",
    "seed:memberships:prod": "npx ts-node -r tsconfig-paths/register scripts/seedMembershipPlans.prod.ts",
    "seed:services": "npx ts-node -r tsconfig-paths/register scripts/seedServices.ts",
    "seed:all": "npm run seed:admin && npm run seed:memberships && npm run seed:services",
    "seed:all:prod": "npm run seed:admin && npm run seed:memberships:prod && npm run seed:services",
    "check": "npm run lint && npm run typecheck"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.884.0",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.6.0",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "file-type": "^21.0.0",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.17",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "pino": "^9.10.0",
    "stripe": "^18.5.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/stripe": "^8.0.416",
    "@types/winston": "^2.4.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
