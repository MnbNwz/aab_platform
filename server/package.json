{
  "name": "server",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "dev": "npm cache clean --f && reset && nodemon -r tsconfig-paths/register app.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "seed:admin": "npx ts-node -r tsconfig-paths/register scripts/seedAdmin.ts",
    "seed:memberships": "npx ts-node -r tsconfig-paths/register scripts/seedMembershipPlans.ts",
    "seed:all": "npm run seed:admin && npm run seed:memberships",
    "seed:payment": "npx ts-node -r tsconfig-paths/register scripts/seedPayment.ts",
    "postman:setup": "./postman/setup.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.884.0",
    "@types/jsonwebtoken": "^9.0.10",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "stripe": "^18.5.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/stripe": "^8.0.416",
    "@types/winston": "^2.4.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
